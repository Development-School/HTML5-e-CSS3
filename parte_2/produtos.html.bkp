<!DOCTYPE html>
<html lang="PT-BR">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Produtos - Barbearia Alura</title>
  <link rel="stylesheet" href="css/produtos.css">
</head>
<body>
  <header>
    <h1><img src="img/logo.png" alt=""></h1>
    <ul id="listaTitulo"></ul>
  </header>
  
  <script>
    var dadosTitulo = ['Home', 'Produtos', 'Contato'];
    var dadosUrl = ['home.html', 'produtos.html', 'contato.html'];
    var listaTitulo = document.getElementById("listaTitulo");

    dadosTitulo.forEach((tempItem) => {
      var li = document.createElement("li");
      //li.innerText = tempItem;
      for(var i = 0; i < dadosUrl.length; i++) {
        li.innerHTML = '<a href="'+ dadosUrl[i] + '">' + tempItem + '</a>';
      }
      listaTitulo.appendChild(li);
    });
  </script>
</body>
</html>